[web]
%{ for vm in web_vms ~}
${vm.name} ansible_host=${vm.network_interface[0].nat_ip_address}
%{ endfor ~}

[db]
%{ for vm in db_vms ~}
${vm.name} ansible_host=${vm.network_interface[0].nat_ip_address}
%{ endfor ~}

[storage]
%{ for vm in storage_vms ~}
${vm.name} ansible_host=${vm.network_interface[0].nat_ip_address}
%{ endfor ~}

[all:vars]
ansible_ssh_user=${ssh_user}
ansible_ssh_private_key_file=${ssh_key}